        "sass-mq": "^5.0.1",
        "sass-svg-uri": "^1.0.0"
    },
    "devDependencies": {
        "autoprefixer": "^6.7.7",
        "css-loader": "^3.2.0",
        "highlight.js": "^9.12.0",
